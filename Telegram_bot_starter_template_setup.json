{
  "name": "Telegram Bot Setup",
  "nodes": [
    {
      "parameters": {
        "updates": ["*"]
      },
      "id": "trigger-node",
      "name": "Telegram Trigger",
      "type": "n8n-nodes-base.telegramTrigger",
      "typeVersion": 1,
      "position": [380, 240],
      "webhookId": "96a20e88-ba8f-4827-b874-b0a9867c59e9",
      "credentials": {
        "telegramApi": {
          "id": "telegram-account",
          "name": "Telegram account"
        }
      }
    },
    {
      "parameters": {
        "chatId": "={{ $('Telegram Trigger').item.json.message.from.id }}",
        "text": "Photo"
      },
      "id": "telegram-photo",
      "name": "Send Photo Message",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "position": [700, 240],
      "credentials": {
        "telegramApi": {
          "id": "telegram-account",
          "name": "Telegram account"
        }
      }
    },
    {
      "parameters": {
        "rules": {
          "values": [
            {
              "conditions": {
                "options": {
                  "caseSensitive": true
                },
                "conditions": [
                  {
                    "operator": {
                      "type": "string",
                      "operation": "contains"
                    },
                    "leftValue": "={{ $('Telegram Trigger').item.json.message.text }}",
                    "rightValue": "/command1"
                  }
                ],
                "combinator": "and"
              },
              "renameOutput": true,
              "outputKey": "Command 1"
            },
            {
              "conditions": {
                "options": {
                  "caseSensitive": true
                },
                "conditions": [
                  {
                    "operator": {
                      "type": "string",
                      "operation": "contains"
                    },
                    "leftValue": "={{ $('Telegram Trigger').item.json.message.text }}",
                    "rightValue": "/command2"
                  }
                ],
                "combinator": "and"
              },
              "renameOutput": true,
              "outputKey": "Command 2"
            }
          ]
        }
      },
      "id": "switch-commands",
      "name": "Command Switch",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [1100, 240]
    },
    {
      "parameters": {
        "chatId": "={{ $('Telegram Trigger').item.json.message.from.id }}",
        "text": "Unknown Command"
      },
      "id": "unknown-command",
      "name": "Unknown Command Response",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "position": [1500, 240],
      "credentials": {
        "telegramApi": {
          "id": "telegram-account",
          "name": "Telegram account"
        }
      }
    }
  ],
  "connections": {
    "Telegram Trigger": {
      "main": [
        [
          {
            "node": "switch-commands",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "switch-commands": {
      "main": [
        [
          {
            "node": "telegram-photo",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "unknown-command",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {},
  "id": "telegram-bot-setup"
}
